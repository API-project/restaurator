<div class="container">

  <div class="row">

    <div class="col s10 pull-s2">

<div class="row">
  <!-- <img class="logo-just" src="http://www.brandemia.org/sites/default/files/sites/default/files/logo_just-eat-despues.jpg"> -->
      <div id="search-title" class=" col s5 push-s5"><p> Buscar por: </p></div>
      <div class=" col s12 push-s2" id="locationField">
        <input id="autocomplete" placeholder="Chamberi, Chamartin ..." type="text" />
      </div>
      <button class="btn col s2 push-s3 orange darken-3"><a class="text" href="/">Más info</a></button>
</div>
      <div id="map"></div>



      <div id="listing">
        <table id="resultsTable">
          <tbody id="results"></tbody>
        </table>
      </div>
      <div style="display: none">
        <div id="info-content">
          <table>
            <tr id="iw-url-row" class="iw_table_row">
              <td id="iw-icon" class="iw_table_icon"></td>
              <td id="iw-url"></td>
            </tr>
            <tr id="iw-address-row" class="iw_table_row">
              <td class="iw_attribute_name">Address:</td>
              <td id="iw-address"></td>
            </tr>
            <tr id="iw-phone-row" class="iw_table_row">
              <td class="iw_attribute_name">Telephone:</td>
              <td id="iw-phone"></td>
            </tr>
            <tr id="iw-rating-row" class="iw_table_row">
              <td class="iw_attribute_name">Rating:</td>
              <td id="iw-rating"></td>
            </tr>
            <tr id="iw-website-row" class="iw_table_row">
              <td class="iw_attribute_name">Website:</td>
              <td id="iw-website"></td>
            </tr>
          </table>
        </div>
      </div>

    </div>

    <div class="row">
      <p>
          <div class="col s4 pull-s1">
          <div class="card">
            <div class="card-image">
        
                <img class="restaurant-image" src="https://www.chef2.me/assets/fingers_pollo-0d56aa7dcde9d17b14e4cb737e9e14e1e9e029376b97c8764f8cc50383158ac8.jpg">
              </div>
            <div class="card-content">
              <span class="card-title"><p>Chef2Me</p></span>
              <p><span>Rating: </span>4.3</p>
              <p>Calle de Joaquin Maria Lopez, 10 Madrid</p>
              <br>
              <div class="chip">
              <img  src="" alt="">Comida casera</div>
            </div>
            <div class="card-action">
              <a href="https://www.just-eat.es/restaurants-chef2memadrid">Comida Casera</a>
            </div>
          </div>
      </p>
      </div>



    <p>
        <div class="col s4 pull-s1">
        <div class="card">
          <div class="card-image">

              <img src="https://media-cdn.tripadvisor.com/media/photo-s/11/eb/38/e4/photo1jpg.jpg">
            </div>
          <div class="card-content">
            <span class="card-title"><p>Morikaen</p></span>
            <p><span>Rating: </span>4.4</p>
            <p>Calle Hilarión Eslava, 17 Madrid</p>
            <br>
            <div class="chip">
            <img  src="" alt="">Japones</div>
          </div>
          <div class="card-action">
            <a href="https://www.just-eat.es/restaurants-morikaengraloraa">Pedir a domicilio</a>
          </div>
        </div>
    </p>
    </div>



    <p>
        <div class="col s4 pull-s1">
        <div class="card">
          <div class="card-image">

              <img src="https://www.cateringclick.com/img/anuncios/fotos/madrid/p01827/img-0.jpg">
            </div>
          <div class="card-content">
            <span class="card-title"><p>Las tartas de Atelier</p></span>
            <p><span>Rating: </span>4.8</p>
            <p>Calle de Galileo, 93 Madrid</p>
            <br>
            <div class="chip">
            <img  src="" alt="">Postres y meriendas</div>
          </div>
          <div class="card-action">
            <a href="https://www.lastartasdelatelier.com/">Comprar</a>
          </div>
        </div>
    </p>
    </div>
  </div>


    <div class="row">
      <p>
        <% restaurants.forEach((restaurant) => { %>
          <div class="col s4 pull-s1">
          <div class="card">
            <div class="card-image">

              <a href="/restaurants/<%= restaurant._id %>" class="btn-floating halfway-fab waves-effect waves-light orange darken-3">
              Info
              <i class="material-icons">add</i>
              </a>


                <!-- <% if (restaurant.name == 'undefined') { %>
                <img src="https://s3-eu-west-1.amazonaws.com/assets.11870.com/originales/imagenes/3c/3c1523506c3cc8a0b640da99b37898a7">
                  <% } else { %>
                    <% var index = Math.floor(Math.random() * restaurants.length); %>
                      <img src=<%= restaurants[index].imageUrl %>>
                      <% } %> -->
              </div>
            <div class="card-content">
              <span class="card-title"><p><%= restaurant.name %></p></span>
              <p><span>Rating: </span><%= restaurant.rating %></p>
              <p><%= restaurant.direction %></p>
              <br>

              <div class="chip">
              <img  src="" alt=""><%= restaurant.categories %></div>
            </div>
            <div class="card-action">
              <a href=<%= restaurant.href %>>Pedir a domicilio</a>
              <a href="/restaurants/like/<%=restaurant._id%>"><i class="material-icons">favorite</i></a>
            </div>
          </div>
      </p>
      </div>
      <% }) %>
  </div>



</div>

</div>

